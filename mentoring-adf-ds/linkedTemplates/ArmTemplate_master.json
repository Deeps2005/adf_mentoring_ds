						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_ds_properties_typeProperties_sasUri_secretName": {
						"value": "[parameters('AzureBlobStorage_ds_properties_typeProperties_sasUri_secretName')]"
					},
					"LS_ABLB_SRC_Online_properties_typeProperties_sasUri_secretName": {
						"value": "[parameters('LS_ABLB_SRC_Online_properties_typeProperties_sasUri_secretName')]"
					},
					"LS_ASQL_SINK_DataMart_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_SINK_DataMart_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_ASQL_SINK_staging_data_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_SINK_staging_data_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_MSSQL_SRC_CinemaTickets_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_MSSQL_SRC_CinemaTickets_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-ds_ArmTemplate_0')]"
			]
		},
		{
			"name": "mentoring-adf-ds_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-ds_ArmTemplate_1')]"
			]
		},
		{
			"name": "mentoring-adf-ds_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-ds_ArmTemplate_2')]"
